        <param name="color">Gfx.COLOR_WHITE</param>
        <param name="x">66</param>
        <param name="y">66</param>
    </drawable>
    <label id="DateLabel" font="@Fonts.textFont" />
    <label id="DayNumberLabel" font="@Fonts.bigNumberFont" />
    <label id="Field1Icon" font="@Fonts.iconFont" />
    <label id="Field1Label" font="@Fonts.textFont" />
    <label id="Field2Icon" font="@Fonts.iconFont" />
    <label id="Field2Label" font="@Fonts.textFont" />
    <label id="Field3Icon" font="@Fonts.iconFont" />
    <label id="Field3Label" font="@Fonts.textFont" />
    <label id="SecondTenthLabel" font="@Fonts.bigNumberFont" />
    <label id="SecondUnitLabel" font="@Fonts.bigNumberFont" />
    <label id="WeatherIcon" font="@Fonts.weatherFont" />
    <label id="WeatherLabel" font="@Fonts.textFont" />
</layout>
